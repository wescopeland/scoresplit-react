{
  "name": "scoresplit-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom-sixteen",
    "test:unit:ci": "react-scripts test --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "format": "prettier --write . '**/*.{json,md,ts,tsx}'",
    "format:write": "prettier --write . '**/*.{json,md,ts,tsx}'",
    "format:check": "prettier --check . '**/*.{json,md,ts,tsx}'",
    "lint": "eslint --ext js,ts,tsx src"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@nivo/line": "^0.62.0",
    "@reduxjs/toolkit": "^1.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.5.0",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "10.1.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/testing-library__react": "^10.2.0",
    "prettier": "2.0.5",
    "typescript": "~3.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "trailingComma": "none"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/__mocks__",
      "!src/**/index.ts",
      "!src/**/*.model.ts"
    ]
  }
}
